   "source": [
    "def leapyear(year):\n",
    "    if year %4 ==0 and (year %100 !=0 or year %400 ==0):\n",
    "        return(year, \"  is leap year.\")\n",
    "    return(year, \"is not leap year.\")\n",
    "leapyear(2019)"
   ]
